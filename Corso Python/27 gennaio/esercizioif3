Utente_Nome = " "
Password = " "
ID=0
if (Utente_Nome == " " and Password == " " and ID == 0):  
    Utente_Nome = input("Utente Nome:")
    Password = input("Password:")
    ID = int(input("ID:"))
    if (Utente_Nome == " " and Password == " " and ID == 0):
        print('Errore')
    else: 
        print('Tutto ok')
        
        
    
